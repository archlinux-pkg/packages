pkgname='xorg-startx-systemd'

pkgver=1
pkgrel=1
pkgdesc="Service file for running StartX as SystemD user service"
arch=('any')
license=('unknown')

depends=('xorg-xinit')

source=('startx.service')
sha256sums=('SKIP')

package() {
  install -Dm 755 'startx.service' "${pkgdir}/usr/lib/systemd/user/startx.service"
}
